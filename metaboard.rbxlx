					<ProtectedString name="Source"><![CDATA[local CollectionService = game:GetService("CollectionService")
local Common = game:GetService("ReplicatedStorage").MetaBoardCommon
local Config = require(Common.MetaBoardConfig)

local DrawLineRemoteEvent = Common.DrawLineRemoteEvent
local EraseLineRemoteEvent = Common.EraseLineRemoteEvent
local UndoCurveRemoteEvent = Common.UndoCurveRemoteEvent

MetaBoard = {}
MetaBoard.__index = MetaBoard

function MetaBoard.Init()

  for _, board in ipairs(CollectionService:GetTagged(Config.BoardTag)) do
    MetaBoard.InitBoard(board)
  end

  CollectionService:GetInstanceAddedSignal(Config.BoardTag):Connect(MetaBoard.InitBoard)

  DrawLineRemoteEvent.OnServerEvent:Connect(function(player, board, start, stop, thicknessYScale, color, curveName)
    MetaBoard.DrawWorldLine(player, board, start, stop, thicknessYScale, color, curveName)
  end)
  
  EraseLineRemoteEvent.OnServerEvent:Connect(function(player, board, start, stop, thicknessYScale, color, curveName)
    MetaBoard.EraseWorldLine(board, start, stop, thicknessYScale, color, curveName)
  end)

  UndoCurveRemoteEvent.OnServerEvent:Connect(function(player, board, curveName)
    MetaBoard.DeleteWorldCurve(board, curveName)
  end)

  print("MetaBoard Server initialized")
end

function MetaBoard.InitBoard(board)

  if board:FindFirstChild("Curves") == nil then
      local curvesFolder = Instance.new("Folder")
      curvesFolder.Name = "Curves"
      curvesFolder.Parent = board
  end
  
  if board:FindFirstChild("CurrentZIndex") == nil then
    local CurrentZIndex = Instance.new("NumberValue")
    CurrentZIndex.Value = 0
    CurrentZIndex.Name = "CurrentZIndex"
    CurrentZIndex.Parent = board
  end
end

local function lerp(a, b, c)
	return a + (b - a) * c
end

function MetaBoard.CreateWorldLine(worldLineType, board, start, stop, thicknessYScale, color, curveName, zIndex)

  -- TODO dealing with aspect ratio is gross, figure out square coordinates, similar to GUI

  if worldLineType == "HandleAdornments" then
    local aspectRatio = board.Size.X / board.Size.Y
    local yStuds = board.Size.Y

    local vector = stop - start
    local length = vector.Magnitude * aspectRatio
    local centre = (start + stop)/2
    local rotationRadians = math.atan2(vector.Y, vector.X)

    local boxHandle = Instance.new("BoxHandleAdornment")
    boxHandle.SizeRelativeOffset =
      Vector3.new(
        lerp(1,-1,centre.X/aspectRatio),
        lerp(1,-1,centre.Y),
        -- TODO figure out why this is negative
        -1 - (Config.WorldLine.ZThicknessStuds / board.Size.Z) - Config.WorldLine.StudsPerZIndex * zIndex)
    boxHandle.Size =
      Vector3.new(
        (length / aspectRatio) * yStuds,
        thicknessYScale * yStuds,
        Config.WorldLine.ZThicknessStuds)
    boxHandle.CFrame = CFrame.Angles(0,0,rotationRadians)
    boxHandle.Color3 = color
    
    boxHandle:SetAttribute("Start", start)
    boxHandle:SetAttribute("Stop", stop)
    boxHandle:SetAttribute("ThicknessYScale", thicknessYScale)
    boxHandle:SetAttribute("Color", color)
    

    local startHandle = Instance.new("CylinderHandleAdornment")
    startHandle.SizeRelativeOffset =
      Vector3.new(
        lerp(1,-1,start.X/aspectRatio),
        lerp(1,-1,start.Y),
        -1 - (Config.WorldLine.ZThicknessStuds / board.Size.Z) - Config.WorldLine.StudsPerZIndex * zIndex)
    startHandle.Radius = thicknessYScale / 2 * yStuds
    startHandle.Height = Config.WorldLine.ZThicknessStuds
    startHandle.Color3 = color

    local stopHandle = Instance.new("CylinderHandleAdornment")
    stopHandle.SizeRelativeOffset =
      Vector3.new(
        lerp(1,-1,stop.X/aspectRatio),
        lerp(1,-1,stop.Y),
        -1 - (Config.WorldLine.ZThicknessStuds / board.Size.Z) - Config.WorldLine.StudsPerZIndex * zIndex)
    stopHandle.Radius = thicknessYScale / 2 * yStuds
    stopHandle.Height = Config.WorldLine.ZThicknessStuds
    stopHandle.Color3 = color

    startHandle.Parent = boxHandle
    stopHandle.Parent = boxHandle
    -- TODO bad, this function should just make the line, not addornee/parent it
    startHandle.Adornee = board
    stopHandle.Adornee = board

    return boxHandle
  end

  error(worldLineType.." world line type not implemented")
end

function MetaBoard.DrawWorldLine(player, board, start, stop, thicknessYScale, color, curveName)
  local zIndex
  local curve = board.Curves:FindFirstChild(curveName)
  if curve == nil then
    curve = Instance.new("Folder")
    curve.Name = curveName
    curve:SetAttribute("AuthorUserId", player.UserId)
    curve.Parent = board.Curves

    board.CurrentZIndex.Value += 1
  end

  zIndex = board.CurrentZIndex.Value

  local lineHandle = MetaBoard.CreateWorldLine("HandleAdornments", board, start, stop, thicknessYScale, color, curveName, zIndex)

  lineHandle.Adornee = board
  lineHandle.Parent = curve
end

function MetaBoard.EraseWorldLine(board, start, stop, thicknessYScale, color, curveName)
  local curve = board.Curves:FindFirstChild(curveName)
  if curve == nil then
    error(curveName.." not found")
  end

  for _, lineHandle in ipairs(curve:GetChildren()) do
    if lineHandle:GetAttribute("Start") == start and
        lineHandle:GetAttribute("Stop") == stop and
        lineHandle:GetAttribute("ThicknessYScale") == thicknessYScale then

      lineHandle:Destroy()

      if #curve:GetChildren() == 0 then
        curve:Destroy()
      end
      return
    end
  end
end

function MetaBoard.DeleteWorldCurve(board, curveName)
  local curve = board.Curves:FindFirstChild(curveName)

  if curve then
    curve:Destroy()
  end
end

return MetaBoard]]></ProtectedString>
